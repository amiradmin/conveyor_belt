# Backend API URL
REACT_APP_API_URL=http://localhost:8000/api/camera

# WebSocket URL
REACT_APP_WS_URL=ws://localhost:8000/ws

# Environment
NODE_ENV=development

# Debug mode
REACT_APP_DEBUG=true

# Feature flags
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_VIDEO_STREAM=true
REACT_APP_ENABLE_REALTIME_ANALYSIS=true

# API Timeout (ms)
REACT_APP_API_TIMEOUT=30000

# Video stream settings
REACT_APP_VIDEO_STREAM_URL=http://localhost:8080/live
REACT_APP_RTSP_TO_WEB_SERVER=http://localhost:9999

# Camera defaults
REACT_APP_DEFAULT_CAMERA_COUNT=6
REACT_APP_MAX_CAMERA_COUNT=12

# UI Settings
REACT_APP_THEME=dark
REACT_APP_LANGUAGE=fa
REACT_APP_TIMEZONE=Asia/Tehran
REACT_APP_DATE_FORMAT=jYYYY/jMM/jDD


SKIP_PREFLIGHT_CHECK=true
DISABLE_ESLINT_PLUGIN=true